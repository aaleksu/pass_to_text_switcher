{
  "name": "Password to text",
  "version": "0.1.2",
  "manifest_version": 2,
  "icons": {
	  "16": "img/icon16.png",
	  "48": "img/icon48.png",
	  "128": "img/icon128.png"
  },
  "permissions": [
	  "tabs", "http://*/*", "https://*/*"
  ],
  "background": { "scripts": ["background.js"] },
  "browser_action": {
	  "default_title": "Password field is bad",
	  "default_icon" : "img/default.png",
      "default": "popup.html"
  }
}
